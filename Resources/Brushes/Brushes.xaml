<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="StatusOnlineBrush"
                     Color="LimeGreen"
                     po:Freeze="True" />

    <SolidColorBrush x:Key="StatusOfflineBrush"
                     Color="Red"
                     po:Freeze="True" />

    <SolidColorBrush x:Key="WindowBackground"
                     Color="#F7FFFD"
                     po:Freeze="True" />


    <SolidColorBrush x:Key="TitleColor"
                     Color="#55E6C1"
                     po:Freeze="True" />   
    
    <SolidColorBrush x:Key="MainColor"
                     Color="#2FC4B2"
                     po:Freeze="True" />
    <SolidColorBrush x:Key="ActiveColor"
                     Color="#22AA98"
                     po:Freeze="True" />

</ResourceDictionary>